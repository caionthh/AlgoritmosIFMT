DECLARE Vetor[3] NUMERICO
DECLARE cont NUMERICO
ALGORITMO
ENQUANTO cont < 3
escreva("Entre com trÃªs valores")
LEIA (Vetor[cont])
FIM_ENQUANTO

OrdemNumerica(Vetor)

FIM_ALGORITMO

SUB_ROTINA OrdemNumerica(A[3] Numerico)
	DECLARE aux NUMERICO
	DECLARE cont NUMERICO
	cont <- 1
	ENQUANTO cont < 3
		SE A[cont] > A[cont+1]
			aux <- A[cont]
			A[cont] <- A[cont+1]
			A[cont+1] <- aux
		FIM_SE
	FIM_ENQUANTO	
FIM_SUB_ROTINA


